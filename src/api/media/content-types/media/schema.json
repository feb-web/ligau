{
  "kind": "collectionType",
  "collectionName": "medias",
  "info": {
    "singularName": "media",
    "pluralName": "medias",
    "displayName": "Media"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Titulo": {
      "type": "string"
    },
    "Tipo": {
      "type": "enumeration",
      "enum": [
        "Entrenamiento",
        "Partido",
        "Evento",
        "Otro"
      ]
    },
    "Imagen": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "tags": {
      "type": "string"
    },
    "fecha": {
      "type": "date"
    },
    "idpartido": {
      "type": "string"
    },
    "visible": {
      "type": "boolean"
    },
    "partido": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::partido.partido",
      "inversedBy": "media"
    }
  }
}
